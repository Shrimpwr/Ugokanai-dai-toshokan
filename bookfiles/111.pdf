                            $('#sentToEmailInfoKindle').html(b.errors.message());
                            $('#sendToGoogleDriveMessage').html(b.errors.message());
                        });

                $('#sentToEmailInfoKindle').show('fast');
                $(this).replaceWith('[sent to kindle]');
            });

