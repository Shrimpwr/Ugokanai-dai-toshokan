                    $('#bookDescriptionBox').css('height', 'auto');
                    $('#bookDescriptionBox').css('overflow', 'auto');
                    $('.moreBtn').remove();
                });

                $('#btnSaveBook').click(function () {
                    const success = function (response) {
                        $('#btnSaveBook').addClass('hidden')
                        $('#btnUnsaveBook').removeClass('hidden')
